<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<%= link_to "All Users ", users_path, :class=>'action show' if session[:admin]==true %><br>
<%= link_to "Manage account", edit_user_path(session[:user_id]), :class=>'action show' %><br>
<%= link_to "Add friend", new_friendship_path, :class=>'action new' %><br>
<%= link_to "Create new post", new_user_post_path(session[:user_id]), :class=>'action new' %><br>
<%= link_to "My Account", user_path(session[:user_id]), :class=>'action new' %><br>

<% @posts.each do |post| %>
      <br><br><%= link_to post.title, user_post_path(post.user_id, post.id) %></td>
      <%= link_to post.user.username, user_path(post.user_id)   %></td><br>
<% end %>

