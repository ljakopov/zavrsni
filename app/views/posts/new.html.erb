<h1>Create Post</h1>
<div class="new post">
<%= form_for :post, url: user_posts_path(), html: {multipart: true} do |f| %>
      <p>
        <%= f.label :title, "Naslov: " %>
        <%= f.text_field :title, class:"form-control"  %>
      </p>
      <p>
        <%= f.label :image, "Slika: " %>
        <%= f.file_field :image %>
      </p>
      <p>
        <%= f.submit %>
      </p>
  <% end %>
</div>