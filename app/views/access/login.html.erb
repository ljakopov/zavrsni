<h1>Login</h1>
<%= form_tag(:action=> 'attempt_login') do %>
    <p>
      <%= label_tag(:username) %>
      <%= text_field_tag(:username) %>
    </p>
    <p>
      <%= label_tag(:password) %>
      <%= password_field_tag(:password) %>
    </p>
    <p>
      <%= submit_tag("Log In") %>
    </p>
<%end%>